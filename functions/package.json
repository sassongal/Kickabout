{
  "name": "functions",
  "description": "Cloud Functions for Kattrick",
  "scripts": {
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "mocha --require @babel/register --timeout 10000 'test/**/*.test.js'",
    "test:watch": "mocha --require @babel/register --watch 'test/**/*.test.js'",
    "test:coverage": "nyc --reporter=html --reporter=text mocha --require @babel/register 'test/**/*.test.js'",
    "create-super-admin": "node scripts/createSuperAdmin.js",
    "backfill-geohashes": "node scripts/backfill_geohashes.js"
  },
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/firestore": "^7.10.0",
    "@sentry/node": "^8.11.1",
    "axios": "^1.7.9",
    "firebase-admin": "^13.0.1",
    "firebase-functions": "^7.0.2",
    "geofire-common": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/register": "^7.28.3",
    "chai": "^4.5.0",
    "firebase-functions-test": "^3.4.1",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "sinon": "^19.0.5"
  },
  "private": true
}
